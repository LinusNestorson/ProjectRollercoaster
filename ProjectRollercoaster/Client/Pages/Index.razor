@page "/"
@inject IFeedService FirstFeedService
@attribute [Authorize]

<PageTitle>Index</PageTitle>

<h1 class=indexHeader>Todays news</h1>
<div class="articleGrid">
    <div class="articleOne">
     <p>@feed.PublishDate</p>
    </div>
    <article class="articleTwo">
        <h1></h1>
    </article>
    <article class="articleThree">
        <h1></h1>
    </article>
</div>

@code {
    private Feed feed = new();

    protected override async Task OnInitializedAsync()
    {
        feed = await FirstFeedService.GetFeed();
    }
}
